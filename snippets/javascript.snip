# Log {
snippet l
abbr consoleLog
	console[console.debug ? 'debug' : 'log']("$1:", ${1:});${0}
snippet l'
abbr consoleMark
	console[console.debug ? 'debug' : 'log']('-- ${1:} ----------------');
snippet lc
abbr consoleCount
	console.count('${1}');
# }

# Require {
snippet req
abbr require
