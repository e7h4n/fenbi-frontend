# request
snippet request
	var loading = require('../../common/util/Message').loading('${1}');

	request.always(function () {
		loading.hide();
	});

	request.done(function (resp) {
		console[console.debug ? 'debug' : 'log']("resp:", resp);
	});

	request.fail(function (xhr) {
		require('../../common/util/ExceptionUtil').alert(null, {
			xhr: xhr
		});
	});
# }
